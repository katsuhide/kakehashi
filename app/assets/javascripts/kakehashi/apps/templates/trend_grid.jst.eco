<% image_url_full = @getImageURL(@) %>
<div class='trend-grid'>
	<div class='img-area'>
		<img src=<%= image_url_full %> >
	</div>
	<div class='name-area'>
		<div class='trend-item'>
			#<%= @getRank(@) %>:
		</div>
		<div class='trend-item'>
			<%= @['name'] %>
		</div>
		<div>
			<i class="fa fa-twitter fa-lg"></i><%= @getCount(@) %>
		</div>
	</div>
</div>